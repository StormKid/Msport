<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:id="@+id/coach_all_contain"
    >
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/refresh_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


    <com.msport.clientmaster.view.ObservableScrollExpendableView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/coach_contain"
        android:scrollbars="none"
        android:listSelector="@color/transparent"
        android:divider="@color/transparent"
        android:childDivider="@color/transparent"
        />
    </android.support.v4.widget.SwipeRefreshLayout>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/hint_show"
            android:background="@color/bg_black_trans"
            android:visibility="gone"
            />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/show_dialog"
        />

    <include layout="@layout/share_foot"

        />

</RelativeLayout>