<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:gravity="center"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_high"
        android:padding="@dimen/dp_12"
        android:orientation="vertical"
        >

        <TextView
            android:id="@+id/qianbao_list_billid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="支出订单：12039848473738384"
            android:textSize="@dimen/size_17"
            android:textColor="@color/text_black_2f"
            />
        <TextView
            android:id="@+id/qianbao_list_paymoney"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="支付成功：¥13.30"
            android:textSize="@dimen/size_15"
            android:textColor="@color/text_black_5c"
            android:layout_marginBottom="@dimen/dp_12"
            />
        <TextView
            android:id="@+id/qianbao_list_paytime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="2015年8月18日  18:08"
            android:textSize="@dimen/size_15"
            android:textColor="@color/text_black_5c"
            />


    </LinearLayout>
    <View style="@style/myLine"
        android:layout_marginLeft="@dimen/dp_12"
        />

</LinearLayout>